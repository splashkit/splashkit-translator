<%#
  Record declaration from structs
%>
<%
  @structs.each do |struct|
%>
type <%= struct[:name].type_case %> = record
<%
    # Convert each field type into Pascal type
    struct[:fields].each do |field_name, field_data|
%>
  <%= sk_struct_field_for(field_name, field_data) %>;
<%
    end # end fields.each
%>
end;

operator = (const left, right: <%= struct[:name].type_case %>): Boolean;
operator <> (const left, right: <%= struct[:name].type_case %>): Boolean;
<%
  end # end structs.each
%>
