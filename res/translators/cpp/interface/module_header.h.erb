<%
header = "__#{@header_name}_h"
%>
#ifndef <%= header %>

#define <%= header %>


<%
  dependent_headers.each do |header|
%>
#include "<%= header %>.h"
<%
  end # end dependent_headers.each
%>
#include <string>
#include <vector>
#include <cmath>
#include <cstdint>
using std::string;
using std::vector;

<%#
    Define macros for the header here
%>
<%
  @defines.each do |define|
%>
#define <%= define[:name] %> <%= define[:definition] %>

<%
  end # defines.each
%>
<%= read_template 'interface/types' %>
<%= read_template 'interface/functions' %>

#endif /* <%= header %> */
